   "source": [
    "c = 0 #seed c \n",
    "a = 1 #multiplicative constant a\n",
    "b = 3 #additive constant b\n",
    "m = 10 # modulus\n",
    "cnt = 12 #Length of sequence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0 3 6 9 2 5 8 1 4 7 0 3 6 "
     ]
    }
   ],
   "source": [
    "Xp = c\n",
    "print(Xp,end=' ')\n",
    "while cnt > 0:\n",
    "    Xi = (a * Xp + b) % m\n",
    "    print(Xi,end=' ')\n",
    "    if Xp == Xi:\n",
    "        break\n",
    "    Xp = Xi\n",
    "    cnt -= 1"
   ]
  }
